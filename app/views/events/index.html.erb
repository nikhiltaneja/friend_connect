<div class="row">
  <div class="col-md-9">

    <h2>What your friends want to do</h2>

    <table class="table table-hover">
      <%= render partial: "events/events_table_header" %>

      <%= render partial: "events/event", collection: @events %>
    </table>
  </div>

  <div class="col-md-3">
    <h2>Create an event</h2>
    <%= render 'form' %>
  </div>
</div>

<h2>Your plans</h2>

<table class="table table-hover">
  <%= render partial: "events/events_table_header" %>
  
  <%= render partial: "/events/user_events", collection: @user_events, as: :event %>
</table>



